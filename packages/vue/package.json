{
  "name": "@color-scheme/vue",
  "version": "1.0.0",
  "description": "Vue Plugin Boilerplate",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "esnext": "dist/esnext/index.js",
  "types": "dist/esnext/index.d.ts",
  "files": [
    "dist",
    "src",
    "nuxt"
  ],
  "scripts": {
    "test": "exit 0;",
    "storybook": "start-storybook",
    "precommit": "lint-staged",
    "clean": "rimraf dist",
    "build": "yarn run clean && rollup -c",
    "dev": "rollup -c -w",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint --ext .js,.jsx,.ts,.tsx --fix",
    "lint-all": "yarn run lint .",
    "lint-all:fix": "yarn run lint:fix .",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write",
    "example:kitchensink": "poi -s examples/kitchensink/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/EqualMa/color-scheme.git"
  },
  "keywords": [
    "vue",
    "color sheme",
    "prefers-color-scheme"
  ],
  "author": "Fatih Kadir Akin <fatihkadirakin@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/EqualMa/color-scheme/issues"
  },
  "homepage": "https://github.com/EqualMa/color-scheme#readme",
  "dependencies": {
    "@color-scheme/js": "^1.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.6.4",
    "@babel/preset-env": "^7.6.3",
    "@babel/register": "^7.6.2",
    "@color-scheme/browserslist-config": "1.0.0",
    "@color-scheme/gen-rollup-config": "*",
    "core-js": "^3.4.2",
    "eslint": "^6.1.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-prettier": "^3.1.0",
    "poi": "^12.7.1",
    "rollup": "^1.27.8",
    "vue": "^2.6.10"
  },
  "peerDependencies": {
    "core-js": "^3.4.2",
    "vue": "^2.6.10"
  }
}
